FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    DepotCelling
    {
        type            noSlip;
    }

    DepotWall
    {
        type            noSlip;
    }

    DepotFloor
    {
        type            noSlip;
    }

    DepotPanel12
    {
        type            noSlip;
    }

    DepotWindow
    {
        type            noSlip;
    }

    DepotShutter
    {
        type            noSlip;
    }
}

